<?xml version="1.0" encoding="utf-8"?>
<ripple android:color="?colorControlHighlight"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/mask">
        <shape android:shape="rectangle">
            <solid android:color="@color/sesl_white" />
            <corners android:radius="@dimen/winset_contained_button_corner_radius" />
            <padding android:left="@dimen/winset_contained_button_left_right_padding" android:top="@dimen/winset_contained_button_top_bottom_padding" android:right="@dimen/winset_contained_button_left_right_padding" android:bottom="@dimen/winset_contained_button_top_bottom_padding" />
        </shape>
    </item>
    <item android:id="@id/rec_shape">
        <shape android:tint="?colorButtonNormal" android:shape="rectangle">
            <solid android:color="@color/sesl_white" />
            <corners android:radius="@dimen/winset_contained_button_corner_radius" />
            <padding android:left="@dimen/winset_contained_button_left_right_padding" android:top="@dimen/winset_contained_button_top_bottom_padding" android:right="@dimen/winset_contained_button_left_right_padding" android:bottom="@dimen/winset_contained_button_top_bottom_padding" />
        </shape>
    </item>
</ripple>
